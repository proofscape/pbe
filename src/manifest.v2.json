{
  "manifest_version": 2,
  "name": "Proofscape Browser Extension",
  "version": "(placeholder)",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ]
}
