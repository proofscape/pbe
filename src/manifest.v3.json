{
  "manifest_version": 3,
  "name": "Proofscape Browser Extension",
  "version": "(placeholder)",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ]
}
